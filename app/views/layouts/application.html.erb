<!DOCTYPE html>
<html>
<head>
  <title>Muselisation</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
 <!-- <link href="/assets/favicon.ico" rel="icon" type="image/x-icon" /></head> -->
 <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
<body>


<div class="themes">
	<h1>Muselisation</h1>
	<select class="visual">
		<option value="kaleidoscope">Kaleidoscope</option>
		<option value="galaxy">Galaxy</option>
	</select>
	<div class="message"></div>
	<div class="songName"></div>
</div>

<div class="visualisation"></div>
    <!-- Soundcloud input field -->
	    <div id="playerControls">
        	<div class="instructions">Enter a SoundCloud track or playlist URL.</div>
	        <form id="form" class="form-group">
	            <input id="input" class="form-control" placeholder="https://soundcloud.com/artist/track">
	            <button type="submit" id="submit">  &#9658 </button>
	          <br>
	        </form>
	        <audio id="player" controls autoplay preload autobuffer></audio>
	   </div>


	<script src="//connect.soundcloud.com/sdk.js"></script>


</body>
</html>
